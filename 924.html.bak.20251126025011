    <!-- FontAwesome Icons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    <!-- Using system fonts for offline support -->
    <style>
        /* System font fallback for offline use with wide glyph coverage */
        body {
            font-family: 'Segoe UI', 'Segoe UI Symbol', 'Segoe UI Emoji', 'Noto Sans', 'Noto Sans Symbols', 'Noto Sans Symbols 2', 'Arial Unicode MS', Roboto, 'Helvetica Neue', Arial, system-ui, sans-serif;
        }
    </style>
    <!-- force_empty_dataset removed: using last403 dataset -->
    <!-- Add a safeLetter guard so any opened HTML (file:// or served) never renders '@' for invalid indices -->
    <script>
        if (!window.safeLetter) {
            window.safeLetter = function(n){
                if (typeof n !== 'number' || isNaN(n) || n < 0) return '';
                return String.fromCharCode(65 + n);
            };
        }
    </script>
</head>
